<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Imagenes">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_margin="2dp">

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="106dp"
                android:layout_height="106dp"
                android:contentDescription="@string/saludo"
                android:visibility="visible"
                app:srcCompat="@drawable/montspresa"
                android:layout_gravity="top|left"/>

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="106dp"
                android:layout_height="106dp"
                android:layout_gravity="top|center_horizontal"
                android:contentDescription="@string/saludo"
                android:visibility="visible"
                app:srcCompat="@drawable/m00m"
                tools:visibility="visible" />

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="106dp"
                android:layout_height="106dp"
                android:contentDescription="@string/saludo"
                android:visibility="visible"
                app:srcCompat="@drawable/marbol"
                android:layout_gravity="top|right"
                tools:visibility="visible" />
            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="106dp"
                android:layout_height="106dp"
                android:contentDescription="@string/saludo"
                android:visibility="visible"
                app:srcCompat="@drawable/lucho"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="16dp"
                android:layout_gravity="center_vertical|right"
                tools:visibility="visible" />
            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="106dp"
                android:layout_height="106dp"
                android:contentDescription="@string/saludo"
                android:visibility="visible"
                app:srcCompat="@drawable/myl"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="16dp"
                android:layout_gravity="center"
                tools:visibility="visible" />
            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="106dp"
                android:layout_height="106dp"
                android:contentDescription="@string/saludo"
                android:visibility="visible"
                app:srcCompat="@drawable/shadows"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="16dp"
                tools:visibility="visible" />
            <ImageView
                android:id="@+id/imageView7"
                android:layout_width="106dp"
                android:layout_height="106dp"
                android:contentDescription="@string/saludo"
                android:visibility="visible"
                app:srcCompat="@drawable/myl2"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="16dp"
                android:layout_gravity="center"
                tools:visibility="visible" />
            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="106dp"
                android:layout_height="106dp"
                android:contentDescription="@string/saludo"
                android:visibility="visible"
                app:srcCompat="@drawable/mylsp"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="16dp"
                tools:visibility="visible" />
            <ImageView
                android:id="@+id/imageView9"
                android:layout_width="106dp"
                android:layout_height="106dp"
                android:contentDescription="@string/saludo"
                android:visibility="visible"
                app:srcCompat="@drawable/mylfeli"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="16dp"
                android:layout_gravity="center"
                tools:visibility="visible" />
            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="106dp"
                android:layout_height="106dp"
                android:contentDescription="@string/saludo"
                android:visibility="visible"
                app:srcCompat="@drawable/myl3"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="16dp"
                tools:visibility="visible" />

        </LinearLayout>
    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>